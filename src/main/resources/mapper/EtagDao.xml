<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace = "io.github.tawn0000.curation.dao.EtagDao">
    <select id = "queryEtag" resultType = "io.github.tawn0000.curation.entity.Etag">
        select *
        from [dbo].[e_tag]
        ORDER BY t_id ASC
    </select>

    <select id="queryE_agByid" resultType = "io.github.tawn0000.curation.entity.Etag">
        select *
        from [dbo].[e_tag]
        where  id = #{etagId}
    </select>

    <insert id="insertEtag"
</mapper>